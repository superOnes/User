<!--pages/billdes/billdes.wxml-->
<view class='contentWrap'>
  <view>
    <view>发票内容</view>
    <view>
      <view>发票抬头：{{invoice.title}}</view>
      <view>纳税人编号：{{invoice.dutynum}}</view>
      <view>
        <view>金额：<text>￥{{invoice.money/100}}</text></view>   
        <view>发票性质：纸质发票</view>   
      </view>
      <view>申请时间：{{invoice.submit_time}}</view>
    </view>
  </view>
  <text></text>
  <view>
    <view>快递信息</view>
    <view>
      <view>
        <view>联系人：{{invoice.name}}</view>
        <view>联系方式：{{invoice.mobile}}</view>
      </view>
      <view>
        <view>快递类型：{{expressCon}}</view>   
        <view>快递金额：<text>￥{{invoice.express_expenses/100}}</text></view>   
      </view>
      <view>快递号：{{invoice.express_number}}</view>
    </view>
  </view>
  <text></text>
  <view>
    <view>包含订单数：{{orderLength}}单</view>
    <view wx:for='{{order}}' wx:key='key'>
      <view>订单号：{{item.order_code}}</view>
      <text>￥{{item.deal_price/100}}</text>
    </view>
  </view>
</view>
