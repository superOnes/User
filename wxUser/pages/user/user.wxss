/* pages/user/user.wxss */
.imagesWrap{
  display: block;
  padding: 40rpx 0;
}
.imagesWrap image{
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
}
.imagesWrap text{
  display: block;
  margin: 20rpx 0;
  text-align: center;
}
.settingWrap{
  padding: 0 40rpx;
}
.settingWrap navigator{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  border-bottom: 1px solid #e5e5e5;
  align-items: center;
  padding: 20rpx;
}
.settingWrap navigator text{
  width: calc(100% - 80rpx);
}
.settingWrap navigator icon image{
  display: block;
  width: 15rpx;
  height: 30rpx;
}
.settingWrap navigator icon:first-child image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
}