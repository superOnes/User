/* pages/otherMsg/otherMsg.wxss */
.otherMsg{
  background: #fff;
  padding: 20rpx;
  font-weight: 600;
  color: #333;
}
.otherMsg view:first-child{
  text-indent: 2em;
  font-size: 26rpx;
  line-height: 50rpx;
}
.otherMsg view icon{
  display: block;
  margin-top: 230rpx;
}
.otherMsg view icon image{
  display: block;
  margin: 0 auto;
  width: 200rpx;
  height: 200rpx;
}
.otherMsg view text{
  display: block;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  font-weight: 700;
}
.otherMsg view:last-of-type{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 999;
  background: #fff;
  padding-bottom: 60rpx;
}
.otherMsg view:last-of-type text{
  display: block;
  text-align: center;
  font-size: 26rpx;
  line-height: 50rpx;
}